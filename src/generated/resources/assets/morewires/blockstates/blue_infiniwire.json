{
  "multipart": [
    {
      "apply": {
        "model": "morewires:block/infiniwire_dot"
      },
      "when": {
        "OR": [
          {
            "east": "none",
            "north": "none",
            "south": "none",
            "west": "none"
          },
          {
            "AND": [
              {
                "OR": [
                  {
                    "north": "side|up"
                  },
                  {
                    "south": "side|up"
                  }
                ]
              },
              {
                "OR": [
                  {
                    "east": "side|up"
                  },
                  {
                    "west": "side|up"
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    {
      "apply": {
        "model": "morewires:block/infiniwire_side0"
      },
      "when": {
        "north": "side|up"
      }
    },
    {
      "apply": {
        "model": "morewires:block/infiniwire_side_alt0"
      },
      "when": {
        "south": "side|up"
      }
    },
    {
      "apply": {
        "model": "morewires:block/infiniwire_side_alt1",
        "y": 270
      },
      "when": {
        "east": "side|up"
      }
    },
    {
      "apply": {
        "model": "morewires:block/infiniwire_side1",
        "y": 270
      },
      "when": {
        "west": "side|up"
      }
    },
    {
      "apply": {
        "model": "morewires:block/infiniwire_up"
      },
      "when": {
        "north": "up"
      }
    },
    {
      "apply": {
        "model": "morewires:block/infiniwire_up",
        "y": 90
      },
      "when": {
        "east": "up"
      }
    },
    {
      "apply": {
        "model": "morewires:block/infiniwire_up",
        "y": 180
      },
      "when": {
        "south": "up"
      }
    },
    {
      "apply": {
        "model": "morewires:block/infiniwire_up",
        "y": 270
      },
      "when": {
        "west": "up"
      }
    }
  ]
}